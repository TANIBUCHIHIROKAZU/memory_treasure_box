<h3>カテゴリー</h3>
<%= form_with model: @genre_new,url: admin_genres_path, lacal:true do |f| %>
  <%= f.label :name, "カテゴリー" %>
  <%= f.text_field :name %>
  <div>
  　<%= f.submit "作成する" %>
  </div>
<% end %>

<table>
  <thead>
    <tr>
    </tr>
  </thead>
  <tbody>
        <% @genres.each do |genre| %>
          <tr>
            <td>
              <%= genre.name %>
            </td>
          </tr>
        <% end %>
  </tbody>
</table>